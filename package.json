{
  "name": "vite-plugin-vue-static-sfc",
  "version": "1.0.0",
  "author": "Arik W",
  "license": "MIT",
  "description": "Serve \".vue\" files as static assets",
  "scripts": {
    "dev": "vite",
    "lint": "node_modules/.bin/eslint --ext .js,.vue --max-warnings=0 --ignore-path .eslintignore ."
  },
  "devDependencies": {
    "eslint": "^8.18.0"
  },
  "engines": {
    "node": ">= 12"
  },
  "main": "src/index.js",
  "exports": {
    ".": {
      "import": "./src/index.mjs",
      "require": "./src/index.js"
    }
  },
  "files": [
    "/src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arikw/vite-plugin-vue-static-sfc.git"
  },
  "bugs": {
    "url": "https://github.com/arikw/vite-plugin-vue-static-sfc/issues"
  },
  "homepage": "https://github.com/arikw/vite-plugin-vue-static-sfc#readme",
  "keywords": [
    "vite",
    "vue",
    "sfc",
    "static",
    "vite-plugin"
  ]
}
